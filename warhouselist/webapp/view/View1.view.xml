<mvc:View xmlns:filterbar="sap.ui.mdc.filterbar" xmlns:tnt="sap.tnt" xmlns:core="sap.ui.core" xmlns:table2="sap.ui.mdc.table" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:dnd="sap.ui.core.dnd" xmlns:semantic="sap.m.semantic" xmlns:f="sap.f" xmlns:fb="sap.ui.comp.filterbar" xmlns:svm="sap.ui.comp.smartvariants" xmlns:table="sap.ui.table" controllerName="sap.btp.warhouselist.controller.View1" displayBlock="true"><Page id="_IDGenDynamicPage1"> 
       
        <content>
            <Table id="orderTable" items="{path: 'warehouseData>/'}">
             <headerToolbar>
                <Toolbar>
                    <Title text="Orders"/>
                </Toolbar>
             </headerToolbar>
                <columns>
                    <Column id="_IDGenColumn0">
                        <Text id="indexText" text="Index"/>
                    </Column>
                    <Column id="_IDGenColumn1">
                        <Text id="Warehouse" text="Warehouse"/>
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Text id="WarehouseOrder" text="WarehouseOrder"/>
                    </Column>
                    <Column id="_IDGenColumn3">
                        <Text id="status" text="Status"/>
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Text id="statusName" text="Status Name"/>
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Text id="createdOn" text="Created On"/>
                    </Column>
                    <Column id="_IDGenColumn6">
                        <Text id="_IDGenText1" text="Last Changed On"/>
                    </Column>
                    <Column id="_IDGenColumn7">
                        <Text id="ExecutingResource" text="Executing Resource"/>
                    </Column>
                    <Column id="_IDGenColumn8">
                        <Text id="WhseOrderLatestStartDateTime" text="Latest Start on"/>
                    </Column>
                    <Column id="_IDGenColumn9">
                        <Text id="WhseOrderConfirmedDateTime" text="Confirmed on"/>
                    </Column> 
                </columns>

                <dragDropConfig>
                    <dnd:DragDropInfo id="_IDGenDragDropInfo1" sourceAggregation="items" targetAggregation="items" dropPosition="Between" dragStart="onDragStart" drop="onDrop"/>
                </dragDropConfig>
                
                <ColumnListItem id="_IDGenColumnListItem1">
                    <cells>
                        <ObjectIdentifier id="_IDGenObjectIdentifier0" text="{warehouseData>originalIndex}"/>
                        <ObjectIdentifier id="WarehouseCell" text="{warehouseData>Warehouse}"/>
                        <ObjectIdentifier id="_IDGenObjectIdentifier1" text="{warehouseData>WarehouseOrder}"/>
                        <Text id="_IDGenObjectIdentifier2" text="{path : 'warehouseData>WarehouseOrderStatus'}"/>
                        <ObjectIdentifier id="_IDGenObjectIdentifier3" text="{warehouseData>WarehouseOrderStatusName}"/>
                        <ObjectIdentifier text="{ path : 'warehouseData>CreationDateTime',
                                                  formatter:'.formatter.formatDate'}"/>
                        <ObjectIdentifier text="{ path : 'warehouseData>LastChangeDateTime',
                                                  formatter:'.formatter.formatDate'}"/>                          
                        
                        <ObjectIdentifier id="_IDGenObjectIdentifier5" text="{warehouseData>ExecutingResource}"/>
                        <ObjectIdentifier text="{ path : 'warehouseData>WhseOrderLatestStartDateTime',
                                                  formatter:'.formatter.formatDate'}"/>  
                        <ObjectIdentifier text="{ path : 'warehouseData>WhseOrderConfirmedDateTime',
                                                  formatter:'.formatter.formatDate'}"/>    
                    </cells>
                </ColumnListItem>
        </Table>
        </content>
        <footer>
            <OverflowToolbar id="_IDGenOverflowToolbar1">
                <Button id="_IDGenButton1" icon="sap-icon://message-popup" press="date"/>
                <ToolbarSpacer id="_IDGenToolbarSpacer1"/>
                <Button id="_IDGenButton2" type="Accept" text="Save" press="onSave"/>
                <Button id="_IDGenButton3" type="Reject" text="Cancel" press="onCancel"/>
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>